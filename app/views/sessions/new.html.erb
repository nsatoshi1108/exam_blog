<h1>Log in</h1>
<div class="log_in">
  <%= form_with(scope: :session, url: session_index_path,local: true) do |f|%>
    <p><%= f.label :email%></p>
    <p><%= f.email_field :email%></p>

    <p><%= f.label :password%></p>
    <p><%= f.password_field :password%></p>

    <p><%= f.submit "Log in"%></p>
  <% end%>
</div>
